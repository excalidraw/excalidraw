  "main": "main.js",
  "types": "types/excalidraw/index.d.ts",
  "files": [
    "dist/*",
    "types/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "description": "Excalidraw as a React component",
  "repository": "https://github.com/excalidraw/excalidraw",
