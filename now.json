{
  "redirects": [
    {
      "source": "/([^.]+)",
      "destination": "/",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
