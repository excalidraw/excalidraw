# Application mode (e.g., development, production)
MODE="production"

# Backend API URLs for GET and POST requests
VITE_APP_BACKEND_V2_GET_URL=<BACKEND_V2_GET_URL>
VITE_APP_BACKEND_V2_POST_URL=<BACKEND_V2_POST_URL>

# URLs for the library and its backend
VITE_APP_LIBRARY_URL=<LIBRARY_URL>
VITE_APP_LIBRARY_BACKEND=<LIBRARY_BACKEND>

# URLs for Excalidraw Plus landing page and app
VITE_APP_PLUS_LP=<PLUS_LP>
VITE_APP_PLUS_APP=<PLUS_APP>

# Backend URL for AI-related features
VITE_APP_AI_BACKEND=<AI_BACKEND>

# WebSocket server URL for real-time collaboration
VITE_APP_WS_SERVER_URL=<WS_SERVER_URL>

# Firebase configuration for the app (replace placeholders with actual values)
VITE_APP_FIREBASE_CONFIG='{"apiKey":"<FIREBASE_API_KEY>","authDomain":"<FIREBASE_AUTH_DOMAIN>","databaseURL":"<FIREBASE_DATABASE_URL>","projectId":"<FIREBASE_PROJECT_ID>","storageBucket":"<FIREBASE_STORAGE_BUCKET>","messagingSenderId":"<FIREBASE_MESSAGING_SENDER_ID>","appId":"<FIREBASE_APP_ID>"}'

# Enable or disable tracking (set to false for privacy)
VITE_APP_ENABLE_TRACKING=false

# Public key for Excalidraw Plus export functionality
VITE_APP_PLUS_EXPORT_PUBLIC_KEY=<PLUS_EXPORT_PUBLIC_KEY>

# Debugging flags for development mode (set to false in production)
VITE_APP_DEBUG_ENABLE_TEXT_CONTAINER_BOUNDING_BOX=false
VITE_APP_COLLAPSE_OVERLAY=false

# Enable ESLint checks in the development server
VITE_APP_ENABLE_ESLINT=false
